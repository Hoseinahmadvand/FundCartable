@model FundCartable.ViewModels.FundVM

@{
    ViewData["Title"] = "create";
}
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-controller="Fund" asp-action="CreateFund" method="post">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Fund_Name" class="control-label"></label>
                <input asp-for="Fund_Name" class="form-control" />
                <span asp-validation-for="Fund_Name" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Fund_Address" class="control-label"></label>
                <input asp-for="Fund_Address" class="form-control" />
                <span asp-validation-for="Fund_Address" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Fund_Count" class="control-label"></label>
                <input asp-for="Fund_Count" class="form-control" />
                <span asp-validation-for="Fund_Count" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Fund_Description" class="control-label"></label>
                <input asp-for="Fund_Description" class="form-control" />
                <span asp-validation-for="Fund_Description" class="text-danger"></span>
            </div>
           
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

